	        numeros[1] = 20;
	        numeros[2] = 30;

	       
	        System.out.println("Número 1: " + numeros[0]);
	        System.out.println("Número 2: " + numeros[1]);
	        System.out.println("Número 3: " + numeros[2]);
	
	
	}
	
	
	
	






			
		
		
			
		
		
		
		
		
		

